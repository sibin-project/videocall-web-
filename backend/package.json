{
  "name": "vc-web",
  "version": "1.0.0",
  "description": "Simple web-based video calling application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd ../client && npm start",
    "build": "cd ../client && npm run build",
    "install-all": "npm install && cd ../client && npm install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.1.10"
  },
  "keywords": [
    "webrtc",
    "video-calling",
    "socket.io",
    "react"
  ],
  "author": "",
  "license": "MIT"
}